{
  "name": "beauty-prints",
  "displayName": "Beauty Prints",
  "description": "Insert a stylized print line to make debugging easier.",
  "version": "0.1.0",
  "repository": {
    "url": "https://github.com/fedeegmz/beauty-prints"
  },
  "publisher": "fedeegmz",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Snippets",
    "Debuggers",
    "Other"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "javascript",
        "aliases": [
          "JavaScript",
          "Javascript",
          "javascript"
        ],
        "extensions": [
          ".js",
          ".jsx"
        ],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "typescript",
        "aliases": [
          "TypeScript",
          "Typescript",
          "typescript"
        ],
        "extensions": [
          ".ts",
          ".tsx"
        ],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "python",
        "aliases": [
          "Python",
          "python"
        ],
        "extensions": [
          ".py"
        ],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "dart",
        "aliases": [
          "Dart",
          "dart"
        ],
        "extensions": [
          ".dart"
        ],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "rust",
        "aliases": [
          "Rust",
          "rust"
        ],
        "extensions": [
          ".rs"
        ],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "kotlin",
        "aliases": [
          "Kotlin",
          "kotlin"
        ],
        "extensions": [
          ".kt"
        ],
        "configuration": "./language-configuration.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "10.0.10",
    "@types/node": "22.13.5",
    "@types/vscode": "1.97.0",
    "@typescript-eslint/eslint-plugin": "8.24.1",
    "@typescript-eslint/parser": "8.24.1",
    "@vscode/test-cli": "0.0.10",
    "@vscode/test-electron": "2.4.1",
    "eslint": "9.21.0",
    "prettier": "3.5.2",
    "typescript": "5.7.3"
  }
}
